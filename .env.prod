# 生产环境配置
NODE_ENV=production

# API 基础地址
VITE_API_BASE_URL=https://api.yourapp.com/api
# 应用标题
VITE_APP_TITLE=Vue3 生产环境
# 是否启用 Mock 数据
VITE_USE_MOCK=false
# 是否启用调试模式
VITE_DEBUG=false
# 上传文件大小限制 (MB)
VITE_UPLOAD_SIZE_LIMIT=50
# WebSocket 地址
VITE_WS_URL=wss://ws.yourapp.com
# 静态资源 CDN 地址
VITE_CDN_URL=https://cdn.yourapp.com
# 是否启用 PWA
VITE_USE_PWA=true