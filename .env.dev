# 开发环境配置
NODE_ENV=development
# API 基础地址
VITE_API_BASE_URL=http://localhost:3000/api
# 应用标题
VITE_APP_TITLE=Vue3 开发环境
# 是否启用 Mock 数据
VITE_USE_MOCK=true
# 是否启用调试模式
VITE_DEBUG=true
# 上传文件大小限制 (MB)
VITE_UPLOAD_SIZE_LIMIT=10
# WebSocket 地址
VITE_WS_URL=ws://localhost:3001
# 静态资源 CDN 地址
VITE_CDN_URL=
# 是否启用 PWA
VITE_USE_PWA=false